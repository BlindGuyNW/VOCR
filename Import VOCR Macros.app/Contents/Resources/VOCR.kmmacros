app = Application.currentApplication()
app.includeStandardAdditions = true

cmd = "screencapture ~/Screenshot.png"
app.doShellScript(cmd)
cmd = 'sips -g pixelWidth ~/Screenshot.png|grep -Eo "(\\d+)"'
width = app.doShellScript(cmd)
cmd = 'sips -g pixelHeight ~/Screenshot.png|grep -Eo "(\\d+)"'
height = app.doShellScript(cmd)
hyp = kme.getvariable("ocr").match(/\d+/g)[0]
density = Math.floor(Math.sqrt(Math.pow(width,2)+Math.pow(height,2))/hyp)
console.log("Desktop: "+width+","+height+","+density)

sys = Application("System Events")
p = sys.applicationProcesses.whose({frontmost:true})
win = p.windows[0]
position = win.position()
size = win.size()[0]
console.log(p.name()+" - "+win.name()+": "+size[0]+","+size[1])

cmd = "screencapture -R"+position+","+size+" ~/Screenshot.png"
app.doShellScript(cmd)
cmd = 'sips -g pixelWidth ~/Screenshot.png|grep -Eo "(\\d+)"'
width = app.doShellScript(cmd)
cmd = 'sips -g pixelHeight ~/Screenshot.png|grep -Eo "(\\d+)"'
height = app.doShellScript(cmd)
console.log("Original: "+width+", "+height)

error = ""
try {
//cmd = "/usr/local/bin/convert ~/Screenshot.png -type Grayscale -resize 400% ~/Screenshot.png"
cmd = "/usr/local/bin/convert ~/Screenshot.png -colorspace Gray -density "+density+" -resample 300 ~/Screenshot.png"
app.doShellScript(cmd)
cmd = 'sips -g pixelWidth ~/Screenshot.png|grep -Eo "(\\d+)"'
width = app.doShellScript(cmd)
cmd = 'sips -g pixelHeight ~/Screenshot.png|grep -Eo "(\\d+)"'
height = app.doShellScript(cmd)
console.log("Enlarged: "+width+", "+height)
} catch (err) {
console.log("Error: Can't find imagemagick.\n"+err)
error += "Error: Can't find imagemagick.\n"
}

wscale = width/size[0]
hscale = height/size[1]
console.log("Scale: "+wscale+", "+hscale)

try {
lang = kme.getvariable("ocrLang")
if (lang == "") lang = "eng"
cmd = "/usr/local/bin/tesseract -l "+lang+" ~/Screenshot.png ~/Screenshot hocr"
app.doShellScript(cmd)
} catch (err) {
console.log("Error: Can't find Tesseract.\n"+err)
error += "Error: Can't find Tesseract.\n"
}

try {
cmd = "cat ~/Screenshot.hocr"
str = app.doShellScript(cmd)
console.log(str)
} catch (err) {
console.log("Error: Can't find output file.\n"+err)
error += "Error: Can't find output file.\n"
str = ""
}

if (error == "") vo.output("Ready")
else vo.output(error)

kme.setvariable("OCRLocation", {to:wscale+","+hscale})
kme.setvariable("ocr", {to:str})
