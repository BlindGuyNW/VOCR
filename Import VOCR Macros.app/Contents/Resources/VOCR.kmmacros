} else return a.y&gt;b.y?1:(a.y&lt;b.y?-1:0)
})
ocr = []
line = []
last = 100000
for (d in data) {
if (Math.abs(data[d].y-last&gt;10)) {
ocr.push(line)
line = []
}
line.push(data[d])
last = data[d].y
}
kme.setvariable("ocr", {to:JSON.stringify(ocr)})
kme.setvariable("ocrLocation", {to:"-1,-1"})
kme.playSound(Path("/System/Library/Sounds/Glass.aiff"))
