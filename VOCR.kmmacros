						<string>&lt;script&gt;
function removeOne(element) {
try {
if (element.getAttribute("class") == "ocrx_word") return
var children = element.children
parent = element.parentNode
if (children.length == 1) {
parent.replaceChild(children[0], element)
removeOne(parent.children[0])
} else if (children.length &gt; 1) {
for (var c=0; c&lt;children.length; c++) {
removeOne(children[c])
}
}
} catch(err) {
console.log(err)
}
}

function KMInit() {
try {
point1 = window.KeyboardMaestro.GetVariable('point1')
point2 = window.KeyboardMaestro.GetVariable('point2')
point1 = point1.match(/\d+/g)
point2 = point2.match(/\d+/g)
wscale = point2[0]/point1[0]
hscale = point2[1]/point1[1]

xml = window.KeyboardMaestro.GetVariable('ocr')
parser = new DOMParser()
xml = parser.parseFromString(xml, "text/xml")
words = xml.getElementsByClassName("ocrx_word")
for (i=0; i&lt;words.length; i++) {
word = words[i]
title = word.getAttribute("title")
title = title.match(/\d+/g)
x1 = title[0]/wscale
y1 = title[1]/hscale
x2 = title[2]/wscale
y2 = title[3]/hscale
x = Math.ceil(x1+((x2-x1)/2))
y = Math.ceil(y1+((y2-y1)/2))
words[i].setAttribute("title", x+","+y)
words[i].setAttribute("onclick", "submitIt(this)")
}
removeOne(xml.body.children[0])
window.document.body.innerHTML = xml.body.innerHTML
} catch(err) {
console.log(err)
}
}

function submitIt(element) {
title = element.getAttribute("title")
loc = title.match(/\d+/g)
window.KeyboardMaestro.SetVariable('x', ""+loc[0])
window.KeyboardMaestro.SetVariable('y', ""+loc[1])
window.KeyboardMaestro.Submit('OK')
}
&lt;/script&gt;
