if (Math.abs(data[d].y-last&gt;10)) {
ocr.push(line)
line = []
}
line.push(data[d])
last = data[d].y
}
kme.setvariable("ocr", {to:JSON.stringify(ocr)})
kme.setvariable("ocrLocation", {to:"-1,-1"})
Application("VoiceOver").output("Ready")
